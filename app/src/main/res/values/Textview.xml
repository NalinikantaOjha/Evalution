<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/dp_10</item>
        <item name="android:textColor">#A2A2A2</item>
    </style>
    <style name="button">
        <item name="android:layout_height">@dimen/dp_70</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_margin">@dimen/dp_70</item>
    </style>
    <style name="Edittext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_70</item>
        <item name="android:layout_marginLeft">@dimen/dp_20</item>
        <item name="android:layout_marginRight">@dimen/dp_20</item>
    </style>
</resources>